{
	"name": "insytio-reborn",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"buildclient": "cd frontend && npm run build",
		"runclient": "cd frontend && npm run start",
		"devstart": "nodemon backend/src/index.js -e js --exec babel-node --delay 2",
		"dev": "concurrently --kill-others \"npm run devstart\" \"npm run runclient\"",
		"start": "nodemon backend/src/server.js -e js --exec babel-node",
		"build:start": "NODE_ENV=production node ./build/src/index.js",
		"clean": "rm -rf build && mkdir build",
		"build-server": "babel -d ./build/src ./backend/src -s --copy-files",
		"build": "npm run clean && npm run build-server",
		"start:dev": "npm run build && nodemon ./build/src/index.js",
		"backend": "nodemon backend/src/server.js -e js --exec babel-node --delay 2",
		"test": "mocha --require @babel/register  ./backend/test/*.test.js --timeout 10000",
		"test:coverage": "nyc --reporter=html mocha --require @babel/register  ./backend/test/*.test.js --timeout 10000",
		"generate-docs": "node_modules/.bin/jsdoc --configure ./backend/jsdoc.json --verbose"
	},
	"dependencies": {
		"@babel/cli": "^7.8.7",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.7",
		"@babel/preset-env": "^7.8.7",
		"@babel/runtime": "^7.8.7",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"chai": "^4.2.0",
		"cookie": "^0.4.1",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"crypto": "^1.0.1",
		"crypto-js": "^3.3.0",
		"dotenv": "^8.2.0",
		"ejwt": "^0.1.0",
		"express": "^4.17.1",
		"express-jwt": "^5.3.3",
		"express-rate-limit": "^5.2.3",
		"express-session": "^1.17.1",
		"joi": "^17.2.1",
		"mongodb": "^4.5.0",
		"mongoose": "^6.3.1",
		"node-cache": "^5.1.0",
		"node-modules": "^1.0.1",
		"nvm": "0.0.4",
		"uuid": "^3.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.15.5",
		"@babel/node": "^7.15.4",
		"@babel/plugin-transform-runtime": "^7.8.7",
		"@babel/register": "^7.8.7",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.4.2",
		"babel-register": "^6.26.0",
		"eslint": "latest",
		"eslint-config-airbnb-base": "latest",
		"eslint-config-prettier": "^7.0.0",
		"eslint-plugin-import": "latest",
		"eslint-plugin-prettier": "^3.3.0",
		"jest": "^24.9.0",
		"jsdoc": "^3.6.6",
		"minami": "^1.2.3",
		"mocha": "^3.5.3",
		"nodemon": "^1.19.4",
		"nyc": "^15.1.0",
		"prettier": "^2.2.1",
		"sinon": "^9.0.2"
	}
}
